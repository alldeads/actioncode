{
    "name": "alldeads/actioncode",
    "description": "Action Code for trademarkers",
    "type": "library",
    "keywords": ["laravel-admin", "extension"],
    "homepage": "https://github.com/alldeads/actioncode",
    "license": "MIT",
    "authors": [
        {
            "name": "TradeMarkers LLC",
            "email": "rexter@trademarkers.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "encore/laravel-admin": "~1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Encore\\Action\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "app/Admin/extensions/laravel-admin-ext/phpinfo"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Encore\\Action\\ActionServiceProvider"
            ]

        }
    }
}
